require 'csv'

masiv=[]
i=0
while i<1000
	s=(rand(28)+1).to_s + "/" + (rand(12)+1).to_s + "/" + (rand(151)+1900).to_s
	s1=rand(201).to_f.round(2)
	s2=rand(2001).to_f.round(2)
	while s1>s2
		s2=rand(2001).to_f.round(2)
	end 
	masiv << [s,s1,s2]
	i+=1
end

CSV.open("task2.csv","w") do |csv|
	masiv.each do |element|
		csv << element
	end
end
